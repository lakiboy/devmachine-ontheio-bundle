{
    "name": "devmachine/ontheio-bundle",
    "type": "symfony-bundle",
    "description": "onthe.io image cloud API integration.",
    "keywords": [ "image", "cloud" ],
    "homepage": "http://devmachine.net",
    "license": [ "MIT" ],
    "authors": [
        { "name": "Dmitri Lakachauskis", "email": "dmitri@devmachine.net" }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "~2.8 || ~3.0",
        "symfony/twig-bridge": "~2.8 || 3.0",
        "sensio/buzz-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": { "Devmachine\\Bundle\\OntheioBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
